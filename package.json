    "@nestjs/testing": "6.5.3",
    "@types/jest": "^24.0.11",
    "@types/supertest": "^2.0.7",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "3.4.5"
